const cardbody=document.querySelectorAll(".card-body")[1];//Elementlerin değişeceği yer
const newElement=document.createElement("h3");//Yeni Element Yaratılıyor.
const oldElement=document.querySelector(".card-title");//Eski Elementi Seçtik.


newElement.className="card-title";
newElement.id="tasks-title";
newElement.textContent="Yeni Todolar";

cardbody.replaceChild(newElement,oldElement);



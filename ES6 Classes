class Employee{
    constructor(name, age,salary){
        this.name = name;
        this.age = age;
        this.salary = salary;
    }
    showInfos(){
        console.log("İsim: "+this.name+" Yaş "+this.age+" Maaş: "+this.salary)
    }
}
const emp=new Employee("Çağlar",22,10000);
emp.showInfos();

function Employee(name,age,salary){
    this.name = name;
    this.age = age;
    this.salary = salary;

    this.showInfos=function(){
        console.log(this.name,this.age,this.salary);
    }
}
const emp1=new Employee("Çağlar",21,0);
const emp2=new Employee("Bunyamin",21,0);

emp1.showInfos();
emp2.showInfos();
